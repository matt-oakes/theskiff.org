  they: she
  them: her
  their: her
  image: https://getdrip.s3.amazonaws.com/uploads/image_upload/image/119241/embeddable_5d68b198-ea9a-41fa-a5df-363d5a2d1d43.jpg

week:
  date: "23-10-2017"
  days:
    -
      name: Monday
      events:
        - show_and_tell
    -
      name: Tuesday
      events:
        - dataviz_brighton
    -
