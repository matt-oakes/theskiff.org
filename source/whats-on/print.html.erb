---

<div class="page">
<%= partial "captain", :locals => {first_name: "Margaret", last_name: "Ousby", they: "she", them: "her", their: "her", image: "https://getdrip.s3.amazonaws.com/uploads/image_upload/image/119241/embeddable_5d68b198-ea9a-41fa-a5df-363d5a2d1d43.jpg"} %>
</div>

<% @i = 1 %>
<% current_page.data.week.days.each do |day| %>
  <% day.events.each do |event| %>
    <% unless event.blank? %>
      <div class="page">
        <%= partial "event", :locals => {:event=>data.events[event], :week=>current_page.data.week.date, :day=>day.name, :index=>@i, } %>
      </div>
    <% @i += 1 %>
    <% end %>
  <% end %>
<% end %>