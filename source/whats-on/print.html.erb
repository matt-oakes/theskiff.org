---
title: What's on at The Skiff
layout: "print"
sitemap_noindex: true
sitemap_priority: 0.8

captain:
  first_name: Sim
  last_name: Brody
  they: he
  theyre: he's
  them: him
  their: his
  image: /images/sim-brody.jpg

week:
  date: "27-11-2017"
  days:
    -
      name: Monday
      events:
        - show_and_tell
    -
      name: Tuesday
      events:
        - dataviz_brighton
    -
      name: Wednesday
      events:
        - farm_open_day
        - pilates
        - the_farm
    -
      name: Thursday
      events:
        - imporvements_lunch
    -
      name: Friday
      events:
        - managing_stress
    -
      name: Saturday
      events: []

---

<div class="page">
<%= partial "print_captain", :locals => current_page.data.captain %>
</div>

<% @i = 1 %>
<% current_page.data.week.days.each do |day| %>
  <% day.events.each do |event| %>
    <% unless event.blank? %>
      <div class="page">
        <%= partial "print_event", :locals => {:event=>data.events[event], :week=>current_page.data.week.date, :day=>day.name, :index=>@i, } %>
      </div>
    <% @i += 1 %>
    <% end %>
  <% end %>
<% end %>